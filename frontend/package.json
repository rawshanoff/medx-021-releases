        "i18next-browser-languagedetector": "^8.2.0",
        "jwt-decode": "^4.0.0",
        "lucide-react": "^0.330.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^16.5.4",
        "react-router-dom": "^6.22.0",
        "tailwind-merge": "^3.4.0"
