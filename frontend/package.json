        "eslint-plugin-react-refresh": "^0.4.18",
        "jsdom": "^28.0.0",
        "postcss": "^8.5.6",
        "prettier": "^3.8.1",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.2.2",
        "vite": "^5.1.4",
        "vitest": "^4.0.18"
